{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch BDD cycle backward",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "-S",
                "1",
                "-a",
                "4",
                "${workspaceRoot}/instances/wt040/wt040_001.dat",
                "4"
            ],
            "cwd": "${workspaceRoot}/build",
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
